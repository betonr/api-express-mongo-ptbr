{
  "name": "api-express-mongo-pt-br",
  "version": "0.7.0",
  "description": "API ...",
  "main": "main.js",
  "scripts": {
    "dev": "nodemon --inspect main.js --exec babel-node",
    "start": "npm run docs && npm run clusters",
    "clusters": "NODE_ENV=production babel-node main.js",
    "docs": "apidoc -i routes/doc -o public/doc",
    "test": "NODE_ENV=test babel-node __test__/index.js --logHeapUsage --coverage"
  },
  "apidoc": {
    "name": "Documentação da API ...",
    "template": {
      "forceLanguage": "pt_br"
    }
  },
  "dependencies": {
    "apidoc": "^0.15.1",
    "babel-cli": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "consign": "^0.1.6",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "express-validation": "^1.0.2",
    "formidable": "^1.2.1",
    "helmet": "^3.9.0",
    "joi": "^13.0.1",
    "jsonwebtoken": "^8.0.0",
    "mongoose": "^5.0.6",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "url": "^0.11.0",
    "winston": "^3.0.0-rc3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "regenerator-runtime": "^0.11.1",
    "supertest": "^3.0.0"
  },
  "jest": {
    "globals": {
      "address": "http://localhost:3001"
    },
    "verbose": true,
    "testEnvironment": "node",
    "coverageReporters": [
      "lcov"
    ],
    "coverageDirectory": "./__test__/resultado"
  }
}
